<BorderPane fx:id="pane" prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.131" xmlns:fx="http://javafx.com/fxml/1">
 <left>
  <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" spacing="10.0" BorderPane.alignment="CENTER">
     <children>
        <Label fx:id="generalTab" text="General">
           <font>
              <Font name="Segoe UI Light" size="18.0" />
           </font>
        </Label>
        <Label fx:id="appearanceTab" text="Appearance">
           <font>
              <Font name="Segoe UI Light" size="18.0" />
           </font>
        </Label>
        <Label fx:id="spreadsheetTab" text="Spreadsheet">
           <font>
              <Font name="Segoe UI Light" size="18.0" />
           </font>
        </Label>
     </children>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
         <padding>
            <Insets left="10.0" right="10.0" top="10.0" />
         </padding>
  </VBox>
 </left>
 <center>
  <AnchorPane fx:id="dynamicPane" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets left="10.0" />
         </BorderPane.margin></AnchorPane>
 </center>
 <bottom>
    <GridPane BorderPane.alignment="CENTER">
      <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="363.0" minWidth="10.0" prefWidth="262.0" />
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="363.0" minWidth="10.0" prefWidth="73.0" />
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="203.0" minWidth="10.0" prefWidth="61.0" />
      </columnConstraints>
      <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      </rowConstraints>
       <children>
          <Button fx:id="apply" mnemonicParsing="false" prefHeight="25.0" prefWidth="60.0" text="Apply" GridPane.columnIndex="1" />
          <Button fx:id="ok" mnemonicParsing="false" prefHeight="25.0" prefWidth="61.0" text="OK" GridPane.columnIndex="2" />
          <Button fx:id="cancel" mnemonicParsing="false" text="Cancel" />
       </children>
       <padding>
          <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
       </padding>
    </GridPane>
 </bottom>
